{
  "name": "ethereal/daemon",
  "description": "PhpEthereal: Daemon",
  "keywords": [
    "PhpEthereal",
    "daemon"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-posix-addendum": "*",
    "setbased/exception": "^2.1",
    "setbased/helper-cast": "^2.1",
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Ethereal\\Daemon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ethereal\\Daemon\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
